<h1>Listing adv_pdates</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>P date</th>
      <th>Layout</th>
      <th>Remark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adv_pdates.each do |adv_pdate| %>
      <tr>
        <td><%= adv_pdate.request_id %></td>
        <td><%= adv_pdate.p_date %></td>
        <td><%= adv_pdate.layout %></td>
        <td><%= adv_pdate.remark %></td>
        <td><%= link_to 'Show', adv_pdate %></td>
        <td><%= link_to 'Edit', edit_adv_pdate_path(adv_pdate) %></td>
        <td><%= link_to 'Destroy', adv_pdate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pdate', new_adv_pdate_path %>
