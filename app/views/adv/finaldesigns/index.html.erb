<h1>Listing adv_finaldesigns</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>Upload by</th>
      <th>Remark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adv_finaldesigns.each do |adv_finaldesign| %>
      <tr>
        <td><%= adv_finaldesign.request_id %></td>
        <td><%= adv_finaldesign.upload_by %></td>
        <td><%= adv_finaldesign.remark %></td>
        <td><%= link_to 'Show', adv_finaldesign %></td>
        <td><%= link_to 'Edit', edit_adv_finaldesign_path(adv_finaldesign) %></td>
        <td><%= link_to 'Destroy', adv_finaldesign, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Finaldesign', new_adv_finaldesign_path %>
