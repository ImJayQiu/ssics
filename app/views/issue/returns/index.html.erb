<h1>Listing issue_returns</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>C code</th>
      <th>Qty</th>
      <th>Remark</th>
      <th>Operator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_returns.each do |issue_return| %>
      <tr>
        <td><%= issue_return.date %></td>
        <td><%= issue_return.c_code %></td>
        <td><%= issue_return.qty %></td>
        <td><%= issue_return.remark %></td>
        <td><%= issue_return.operator %></td>
        <td><%= link_to 'Show', issue_return %></td>
        <td><%= link_to 'Edit', edit_issue_return_path(issue_return) %></td>
        <td><%= link_to 'Destroy', issue_return, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Return', new_issue_return_path %>
