<h1>Listing issue_publishes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_publishes.each do |issue_publish| %>
      <tr>
        <td><%= issue_publish.date %></td>
        <td><%= link_to 'Show', issue_publish %></td>
        <td><%= link_to 'Edit', edit_issue_publish_path(issue_publish) %></td>
        <td><%= link_to 'Destroy', issue_publish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publish', new_issue_publish_path %>
